<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ctscan">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CTscan">
        <activity
            android:name=".main.ui.diagnose.SecondProcessActivity"
            android:exported="false" />
        <activity
            android:name=".main.ui.diagnose.ProcessActivity"
            android:exported="false" />
        <activity
            android:name=".main.ui.diagnose.DiagnoseActivity"
            android:exported="false" />
        <activity
            android:name=".main.menu.MenuActivity"
            android:exported="false"
            android:theme="@style/Theme.CTscan.NoActionBar" />
        <activity
            android:name=".main.ui.register.RegisterActivity"
            android:exported="false" />
        <activity
            android:name=".main.ui.main.MainActivity"
            android:exported="false" />
        <activity
            android:name=".main.ui.login.LoginActivity"
            android:exported="false"
            android:theme="@style/Theme.CTscan.NoActionBar" />
        <activity
            android:name=".main.SplashScreen"
            android:exported="true"
            android:theme="@style/Theme.CTscan.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>